{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["ESNext"],
    "outDir": "./dist",
    "removeComments": true,
    "noEmitOnError": true,
    "strict": true,
    "strictNullChecks": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true
  },
  "include": ["./src"]
}